<AnchorPane prefHeight="800.0" prefWidth="1200.0" style="-fx-background-color: #0d1b2a;" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.gui.frontend.GUIController">
